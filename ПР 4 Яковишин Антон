# ПР 4 зад 8.ру
n = int(input())
for i in range(1, n + 1):
    for j in range(1, i + 1):
        print(j,sep='',end='')
    print()
# ПР 4 зад 1.ру
a = int(input())
b = int(input())
for i in range(a, b + 1):
    print(i)
#ПР 4 зад 4.ру
n = int(input())
sum=0
for i in range(n):
    sum += int(input())
print(sum)
#ПР 4 зад 5.ру
n = int(input())
sum=0
for i in range(1, n +1):
    sum += i ** 3
print(sum)
